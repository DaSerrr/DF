{
    "name": "Python & Jupyter Lab",
    "build": {
        "dockerfile": "Dockerfile"
    },
    "settings": {
        "terminal.integrated.shell.linux": "/bin/bash"
    },
    "postCreateCommand": "jupyter lab --allow-root --no-browser --ip=0.0.0.0",
    "forwardPorts": [8888],
    "extensions": [
        "ms-python.python"
    ]
}
